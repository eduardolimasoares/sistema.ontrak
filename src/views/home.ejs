<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="css/mystyle.css">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.6.0/dist/leaflet.css"
   integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ=="
   crossorigin=""/>
    <title>Ontrak Sys</title>
    <script src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js"
    integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew=="
    crossorigin=""></script>
</head>
<body>
   
    <div class="container-fluid map-container">
        <div id="map-ontrak">
        </div>

    </div>





    <div class="container menu-container-left-mobile d-block d-sm-none d-none d-sm-block d-md-none">
        <div class="tag-logo-div">
            <img src="img/tag-log.png" class="img-fluid" alt="">
        </div>
        <div class="row shadow-sm header-menu-div">
            <div class="col-4">
                <input type="image" src="img/car-black.png" class="img-fluid"  alt="">
            </div>
            <div class="col-4">
                <input type="image" src="img/route.png" class="img-fluid"  alt="">
            </div>
            <div class="col-4">
                <input type="image" src="img/sine.png" class="img-fluid"  alt="">
            </div>
        </div>

        <div class="input-group input-group-sm mb-3 div-search-menu">
            <input type="text" class="form-control" placeholder="Procurar Veículos" aria-label="Small" aria-describedby="inputGroup-sizing-sm" style="text-align: center; border-style: none;">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-sm" style="background-color: #fff; border-style: none;">
                <input type="image" src="img/search.png" class="img-fluid"  alt="">
              </span>
            </div>
        </div>

        <div class="group-line">
            <div class="row car-cel-items">
                <div class="col-1" style="align-self: center;">
                    <img src="img/selected.png" alt="">
                </div>
                <div class="col-1 car-img-div">
                    <img src="img/truck.png" alt="">
                </div>
                <div class="col-3 car-item-div">
                    <p class="item-nome-car">Accelo 815</p>
                </div>
                <div class="col-4 car-item-div">
                    <p class="item-placa">OTP-8898</p>
                </div>
                <div class="col-1 car-item-div">
                    <img src="img/ellipse-green.png" alt="">
                </div>
                <div class="col-1 car-item-div">
                    <img src="img/tree-point.png" alt="">
                </div>
            </div>
        </div>


    </div>




    







    <div class="container menu-container-left d-none d-md-block d-lg-none d-none d-lg-block d-xl-none d-none d-xl-block">
        <div class="tag-logo-div">
            <img src="img/tag-log.png" class="img-fluid" alt="">
        </div>
        <div class="row shadow-sm header-menu-div">
            <div class="col-4">
                <input type="image" src="img/car-black.png" class="img-fluid"  alt="">
            </div>
            <div class="col-4">
                <input type="image" src="img/route.png" class="img-fluid"  alt="">
            </div>
            <div class="col-4">
                <input type="image" src="img/sine.png" class="img-fluid"  alt="">
            </div>
        </div>

        <div class="input-group input-group-sm mb-3 div-search-menu">
            <input type="text" class="form-control" placeholder="Procurar Veículos" aria-label="Small" aria-describedby="inputGroup-sizing-sm" style="text-align: center; border-style: none;">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-sm" style="background-color: #fff; border-style: none;">
                <input type="image" src="img/search.png" class="img-fluid"  alt="">
              </span>
            </div>
        </div>

        <div class="group-line">
            <div class="row car-cel-items">
                <div class="col-1" style="align-self: center;">
                    <img src="img/selected.png" alt="">
                </div>
                <div class="col-1 car-img-div">
                    <img src="img/truck.png" alt="">
                </div>
                <div class="col-3 car-item-div">
                    <p class="item-nome-car">Accelo 815</p>
                </div>
                <div class="col-4 car-item-div">
                    <p class="item-placa">OTP-8898</p>
                </div>
                <div class="col-1 car-item-div">
                    <img src="img/ellipse-green.png" alt="">
                </div>
                <div class="col-1 car-item-div">
                    <img src="img/tree-point.png" alt="">
                </div>
            </div>
        </div>


    </div>


<script>


var map = L.map('map-ontrak', {zoomControl: false,}).setView([-1.458223, -48.500804], 16);

L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6ImNpejY4NXVycTA2emYycXBndHRqcmZ3N3gifQ.rJcFIG214AriISLbB6B5aw', {
    zoom:5,
    tap:true,
    attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors, ' +
        '<a href="https://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, ' +
        'Imagery © <a href="https://www.mapbox.com/">Mapbox</a>',
    id: 'mapbox/streets-v11'
}).addTo(map);
L.control.zoom({
    position: 'topright'
}).addTo(map);



</script>
<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
